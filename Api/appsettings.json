    "AllowedHosts": "*",
    "ConnectionStrings": {
        "DefaultConnection": "Data Source={{Host}},{{Port}};User ID={{User}};Password={{Password}};Initial Catalog={{Database}};Integrated Security=False;MultipleActiveResultSets=True;TrustServerCertificate=True"
    }
}
